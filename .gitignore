# Ignore bundler config.
/.bundle


# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore uploaded files in development
/storage/*
!/storage/.keep

/node_modules
/yarn-error.log

/public/assets
.byebug_history

# Ignore master key for decrypting credentials and more.
/config/master.key
log/development.log
tmp/cache/bootsnap-load-path-cache
tmp/cache/assets/sprockets/v3.0/RR/RRiI305vg1wl9k26EJ-O_AZHr5x7_R2-84WPQVY8bBk.cache
